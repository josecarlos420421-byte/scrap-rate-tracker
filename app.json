{
  "expo": {
    "owner": "josecarlos123",
    "name": "Scrap Rate Tracker",
    "slug": "scrap-rate-tracker",
    "version": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "d8740472-9a37-45cf-bf90-cf7eda893c0f"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "scrapratetracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.scrapratetracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A5F7A",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.scrapratetracker.app"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAFAFA",
          "dark": {
            "backgroundColor": "#121212"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
